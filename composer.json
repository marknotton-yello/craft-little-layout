{
    "name": "wbrowar/little-layout",
    "description": "A compact, visual way to lay out fields, elements, and Matrix blocks.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "little layout"
    ],
    "support": {
        "docs": "https://github.com/wbrowar/little-layout/blob/master/README.md",
        "issues": "https://github.com/wbrowar/little-layout/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Will Browar",
            "homepage": "https://wbrowar.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.6.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.6.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "wbrowar\\littlelayout\\": "src/"
        }
    },
    "extra": {
        "name": "Little Layout",
        "handle": "little-layout",
        "developer": "Will Browar",
        "developerUrl": "https://wbrowar.com",
        "documentationUrl": "https://github.com/wbrowar/little-layout/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/wbrowar/little-layout/master/CHANGELOG.md",
        "class": "wbrowar\\littlelayout\\LittleLayout"
    }
}
